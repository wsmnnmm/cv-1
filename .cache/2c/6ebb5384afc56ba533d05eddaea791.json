\n    setTimeout(() => {\r\n        if (string[n] === \"\\n\") {\r\n            //如果是回车就改成换行\r\n            string2 += \"<br>\"\r\n        } else if (string[n] === \" \") {\r\n            string2 += \"&nbsp;\"\r\n        }\r\n        else {\r\n            //如果不是回车就照搬\r\n            string2 += string[n]\r\n        }\r\n\r\n        html.innerHTML = string2\r\n        style.innerHTML = string.substring(0, n) //断点\r\n        window.scrollTo(0, 9999)\r\n        html.scrollTo(0, 9999)\r\n        n = n + 1\r\n        if (n < string.length) {\r\n            //判断是否到末尾，没到就下一步\r\n            step()\r\n        }\r\n    }, 20)\r\n}\r\nstep()\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"edb9e19db600c2dac4dca22941a69670","cacheData":{"env":{}}}